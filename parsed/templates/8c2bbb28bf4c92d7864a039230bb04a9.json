{"components/common/subscription-form":"<div id=\"footerSubcribeForm\" class=\"wow fadeIn\">\r\n    <h5>Sign up & Get 10% off your first order</h5>\r\n    <form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\" id=\"footerSubcribeFormSubmit\">\r\n        <fieldset class=\"form-fieldset\">\r\n            <input type=\"hidden\" name=\"action\" value=\"subscribe\">\r\n            <input type=\"hidden\" name=\"check\" value=\"1\">\r\n            <div class=\"form-field\">\r\n                <div class=\"form-wrap\">\r\n                    <input class=\"form-input\" id=\"nl_first_name\" name=\"nl_first_name\" type=\"text\" value=\"{{customer.name}}\" placeholder=\"{{lang 'newsletter.your_name_placeholder'}}\">\r\n                    <input class=\"form-input\" id=\"nl_email\" name=\"nl_email\" type=\"email\" value=\"{{customer.email}}\" placeholder=\"{{lang 'newsletter.email_placeholder'}}\">\r\n                    <input class=\"btn btn-primary btn-subcribe\" type=\"submit\" value=\"{{lang 'newsletter.subscribe_submit'}}\">\r\n                </div>\r\n            </div>\r\n        </fieldset>\r\n    </form>\r\n</div>\r\n"}
